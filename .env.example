# Constitutional Market Harmonics - Environment Configuration Template
# Copy this file to .env and fill in your actual values
# This file is safe to commit to version control

# JWT Secret for token signing (auto-generated if not set)
JWT_SECRET=your-secure-jwt-secret-here

# Development password for local authentication
DEV_PASSWORD=your-development-password

# API Keys (server-side only - never exposed to client)
ANTHROPIC_API_KEY=your-anthropic-api-key-here

# Server Configuration
PORT=3002

# CORS Origins (comma-separated)
CORS_ORIGINS=http://localhost:3000,http://localhost:3001,http://127.0.0.1:3000